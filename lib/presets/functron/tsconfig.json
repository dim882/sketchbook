{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "noEmitOnError": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "jsxFactory": "jsx",
    "jsxImportSource": "functron",
    "jsxFragmentFactory": "Fragment",
    "strict": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true
  }
}
